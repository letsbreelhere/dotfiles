[user]
  name  = Bree Elle Gardner
  email = letsbreelhere@gmail.com
[branch]
  autosteuprebase = always
[alias]
  amend   = commit --amend --no-edit
  ap      = add -p
  b       = branch
  cb      = rev-parse --abbrev-ref HEAD
  co      = checkout
  comit   = commit
  commmit = commit
  cp      = cherry-pick
  d       = diff
  dc      = diff --cached
  latest  = branch --sort=committerdate
  no      = diff --name-only
  pop     = stash pop
  rc      = rebase --continue
  rs      = rebase --skip
  s       = status
  tree    = log --graph --decorate --pretty=oneline --abbrev-commit
  wip     = !git add -A && git commit -m "WIP"
[commit]
  template = ~/.git-commit-template
[core]
  editor = /usr/local/bin/nvim
  excludesfile = ~/.gitignore
[color]
  branch      = true
  diff        = true
  interactive = true
  status      = true
[push]
  default = current
[clean]
  requireForce = false
[rerere]
  enabled = 1
[help]
  autocorrect = 1
[merge]
  tool = vimdiff
  conflictstyle = diff3
[color "diff"]
  whitespace = red reverse
[diff]
  wsErrorHighlight=new,old
[show]
  wsErrorHighlight=old,new
